{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">
        <div class="nes-container is-centered is-rounded">
            <div class="nes-text is-primary">
                <p>Connexion au Back-Office</p>
            </div>

            {{ form_start(form) }}
            {{ form_row(form.username) }}
            {{ form_row(form.password) }}
            {{ form_errors(form) }}
                <input type="submit" class="nes-btn is-success">
            {{ form_end(form) }}

        </div>
    </div>
{% endblock %}
